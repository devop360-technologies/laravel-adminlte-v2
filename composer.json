{
  "name": "devop360-technologies/laravel-adminlte-v2",
  "description": "Easy AdminLTE integration with Laravel",
  "keywords": ["laravel", "adminlte", "admin", "administrator"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ismail Rehman",
      "email": "ismailrehman0331@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Devop360Technologies\\LaravelAdminLte\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Devop360Technologies\\LaravelAdminLte\\Tests\\": "tests/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Devop360Technologies\\LaravelAdminLte\\AdminLteServiceProvider"
          ]
      }
  },
  "require": {
    "laravel/framework": "^10.0|^11.0|^12.0",
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "orchestra/testbench": "^8.0|^9.0|^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
